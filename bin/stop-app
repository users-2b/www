#!/bin/bash

#====================== configuration start ====================
app=$( grep APP /APP | awk -F= '{ print $2 }' )
#====================== configuration end ======================

[ ! -z $1 ] && site=$1

DATE=`date`
ROOT=/home/www
LOG="$ROOT/log/stopapp.log"

( cd $ROOT/$app;./stopme ) > $LOG 2>&1 &

